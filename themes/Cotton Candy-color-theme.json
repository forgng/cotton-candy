{
  "name": "Cotton Candy",
  "type": "dark",

  "colors": {
    "editor.background": "#24292e",
    "editor.foreground": "#f0efe2",
    "editorError.foreground": "#eb5bcb"
  },
  "tokenColors": [
    {
      "name": "JS import",
      "scope": "keyword.control.import,keyword.control.from,keyword.control.export,keyword.control.default",
      "settings": {
        "foreground": "#ebb1ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JS import variable",
      "scope": "variable.other.readwrite.alias",
      "settings": {
        "foreground": "#68bcd2"
      }
    },
    {
      "name": "JS String",
      "scope": "string.quoted.double,string.quoted.multi,string.quoted.single",
      "settings": {
        "foreground": "#1de9b6",
        "fontStyle": ""
      }
    },

    {
      "name": "JS parenthesis",
      "scope": "punctuation.definition.block,punctuation.section.embedded.begin,punctuation.section.embedded.end,punctuation.definition.parameters.begin.tsx,punctuation.definition.parameters.end.tsx,punctuation.separator.element.python",
      "settings": {
        "foreground": "#faa9e0",
        "fontStyle": ""
      }
    },
    {
      "name": "JS comment",
      "scope": "comment.line.double-slash,comment.line.number-sign.python",
      "settings": {
        "foreground": "#7f848e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS variable type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#DE8FFF"
      }
    },
    {
      "name": "JS variable name",
      "scope": "variable.other.constant,variable.other.readwrite",
      "settings": {
        "foreground": "#FF77AF",
        "fontStyle": ""
      }
    },
    {
      "name": "JS function",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#FFC785",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "JS function",
      "scope": "storage.modifier.async, keyword.control.flow",
      "settings": {
        "foreground": "#FFC785",
        "fontStyle": "italic bold"
      }
    },

    {
      "name": "JS function name",
      "scope": "entity.name.function, meta.definition.function",
      "settings": {
        "foreground": "#81dafc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JS function name",
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#6FF3D6"
      }
    },
    {
      "name": "JS property",
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#FF77AF",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "JS magic number",
      "scope": "constant.numeric.decimal,constant.language.boolean,constant.numeric.dec,constant.other.caps.python",
      "settings": {
        "foreground": "#FFC785",
        "fontStyle": ""
      }
    },
    {
      "name": "JS object",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#FF77AF",
        "fontStyle": ""
      }
    },
    {
      "name": "JS if,else,try,catch",
      "scope": "keyword.control.conditional, keyword.control.trycatch",
      "settings": {
        "foreground": "#9795F1"
      }
    },
    {
      "name": "JS JSX element",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#9795F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JS property",
      "scope": "meta.tag.attributes",
      "settings": {
        "foreground": "#72D6C9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS function parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#DE8FFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS Tag angular bracket",
      "scope": "punctuation.definition.tag.begin,punctuation.definition.tag.end",
      "settings": {
        "foreground": "#DE8FFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JS object key",
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#FF9AA2",
        "fontStyle": ""
      }
    },
    {
      "name": "JS console, Python builtin",
      "scope": "support.class.console,support.function.builtin.python",
      "settings": {
        "foreground": "#FF9AA2"
      }
    },
    {
      "name": "JS Dot accessor",
      "scope": "punctuation.accessor",
      "settings": {
        "foreground": "#f0efe2"
      }
    },
    {
      "name": "JS operator",
      "scope": "keyword.operator.logical,keyword.operator.comparison,keyword.operator.assignment,keyword.operator.ternary,keyword.operator.arithmetic",
      "settings": {
        "foreground": "#72D6C9",
        "fontStyle": ""
      }
    },
    {
      "name": "JSX text",
      "scope": "meta.jsx.children",
      "settings": {
        "foreground": "#f0efe2",
        "fontStyle": ""
      }
    },
    {
      "name": "JS comma",
      "scope": "punctuation.separator.comma",
      "settings": {
        "foreground": "#f0efe2",
        "fontStyle": ""
      }
    },
    {
      "name": "JS Square bracket",
      "scope": "meta.brace.square",
      "settings": {
        "foreground": "#f0efe2",
        "fontStyle": ""
      }
    },

    {
      "name": "JS arrow function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#f0efe2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "TS type",
      "scope": "entity.name.type,support.type.primitive",
      "settings": {
        "foreground": "#FFDAC1"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Python base",
      "scope": "source.python",
      "settings": {
        "foreground": "#f1c6e7"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Python function",
      "scope": "meta.function-call.generic.python",
      "settings": {
        "foreground": "#B19CD9"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Python function",
      "scope": "meta.function-call.arguments.python",
      "settings": {
        "foreground": "#DF7599"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Python function",
      "scope": "punctuation.separator.dict.python",
      "settings": {
        "foreground": "#F6C3E5"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Python function",
      "scope": "keyword.operator.logical.python",
      "settings": {
        "foreground": "#FF77AF"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Python docstring",
      "scope": "string.quoted.docstring.multi.python",
      "settings": {
        "foreground": "#00e5ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python item access argument",
      "scope": "meta.item-access.arguments.python",
      "settings": {
        "foreground": "#f0f696",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python item access",
      "scope": "meta.item-access.python",
      "settings": {
        "foreground": "#9795F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python placeholder",
      "scope": "constant.character.format.placeholder.other.python",
      "settings": {
        "foreground": "#9795F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python cast",
      "scope": "support.type.python",
      "settings": {
        "foreground": "#9795F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON string",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#9795F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON string",
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "#1de9b6",
        "fontStyle": ""
      }
    },

    {
      "name": "TOML table",
      "scope": "entity.other.attribute-name.table.array.toml,entity.other.attribute-name.table.toml",
      "settings": {
        "foreground": "#DF7599",
        "fontStyle": ""
      }
    },
    {
      "name": "TOML table punctuation",
      "scope": "punctuation.definition.table.toml,punctuation.definition.table.array.toml",
      "settings": {
        "foreground": "#FFC785",
        "fontStyle": ""
      }
    },
    {
      "name": "TOML keyword",
      "scope": "keyword.key.toml",
      "settings": {
        "foreground": "#9795F1",
        "fontStyle": "bold"
      }
    }
  ]
}
