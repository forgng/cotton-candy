    {
      "name": "CSS/SCSS property",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#FFC785",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Blad keyword",
      "scope": "keyword.blade",
      "settings": {
        "foreground": "#FFC785",
        "fontStyle": "bold"
      }
